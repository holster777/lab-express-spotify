<div class="track-container">
    <div class="header-bar">
        <h1 class="title">Title</h1>
        <h1 class="listen">Listen</h1>
    </div>
    
        {{#each track}}
        <div class="track-row">
            <h3>{{this.name}}</h3>
            <audio controls>
                <source src="{{this.preview_url}}" type="audio/mpeg">
            </audio>
        </div>
        {{/each}}
    
</div>